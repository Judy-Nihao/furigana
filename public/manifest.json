{
    "manifest_version": 3,
    "name": "Get Furigana",
    "version": "1.0.0",
    "description": "An simple extension that can add Furigana to the selected Japanese texts.",
    "author": "Judy Chen",
    "icons": {
        "128": "icon128.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script-final.js"],
            "css": ["style-final.css"]
        }
    ]
}